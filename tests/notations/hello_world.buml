@startuml
   class A {
      -a1: int
      a2: bool
   }

   abstract class B {
      +b1: int
      #b2: float
      functionB()
   }

   class C {
      +c1: str
      +void functionC()
   }

   A "1" o-- "1..*" B: AggregationAB
   A "1" --o "1" B: AggregationAB2
   A "1" *-- "1" C: CompositionAC
   B "1" -- "1..*" C: BidirectionalBC
   C "1..*" <-- "1..*" A: UnidirectionalAC
   A <|-- B
   C --|> B
@enduml

context A inv a1ConstraintValue:
"self.a1 > 0"

context C inv c1Unique: 
"C.allInstances() -> isUnique(c1)"